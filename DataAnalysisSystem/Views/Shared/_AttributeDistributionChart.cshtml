@model DataAnalysisSystem.DTO.Charts.ChartUniversalViewModel

<script>
    var ctx = document.getElementById('@Model.ChartCanvasId').getContext('2d');

        var myDoughnutChart = new Chart(ctx, {
            type: 'doughnut',
            data: {
                labels: ['Number', 'String'],
                datasets: [{
                    label: '# of Attributes',
                    data: @Html.Raw(Model.Data),
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.2)',
                        'rgba(54, 162, 235, 0.2)'
                    ],
                    borderColor: [
                        'rgba(255, 99, 132, 1)',
                        'rgba(54, 162, 235, 1)'
                    ],
                    borderWidth: 1
                }]
            }
        });
</script>