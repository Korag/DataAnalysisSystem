@model DataAnalysisSystem.DTO.AnalysisDTO.PerformNewAnalysisViewModel

<div class="row" style="margin-top: 2vh; margin-bottom: 2vh;">
    <div class="col s12 m10 l8 offset-m1 offset-l2">
        <div class="card">
            <div class="card-content">
                <span class="card-title center-align">Basic Statistics Method Parameters</span>
                <span class="card-title center teal-text text-lighten-1"></span>

                <div class="input-field">

                    <i class="material-icons prefix">contact_page</i>
                    <input type="text" asp-for="AnalysisParameters.BasicStatisticsParameters.LastName" required="" aria-required="true" class="validate">
                    <label asp-for="AnalysisParameters.BasicStatisticsParameters.LastName"></label>

                    <span asp-validation-for="AnalysisParameters.BasicStatisticsParameters.LastName" class="helper-text" style="color: #F44336"></span>

                </div>

            </div>
        </div>
    </div>
</div>